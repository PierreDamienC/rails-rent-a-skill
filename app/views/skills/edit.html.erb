<div class="container fix_footer">
  <div class="form">
      <h1>Edit a skill</h1>
      <p>Here, create a skill that you will be able to rent later</p>
      <%= simple_form_for(@skill) do |f| %>
        <%= f.input :name %>
        <%= f.input :description %>
        <%= f.input :price %>
        <%= f.input :photo, as: :file %>
        <%= f.button :submit, class: "button" %>
        <% end %>
  </div>
</div>

